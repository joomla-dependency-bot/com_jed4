               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_EXTENSION_UPDATE_LABEL"/>
        <field name="new_version_number" filter="safehtml" type="text"
               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_NEW_VERSION_NUMBER_LABEL"
               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_NEW_VERSION_NUMBER_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_NEW_VERSION_NUMBER_LABEL"/>
