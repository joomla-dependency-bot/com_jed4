               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_VULNERABLE_ITEM_NAME_LABEL"
               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_VULNERABLE_ITEM_NAME_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_VULNERABLE_ITEM_NAME_LABEL"/>
        <field name="vulnerable_item_version" filter="safehtml" type="text" required="true" message="Invalid Input"
