<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Jed\Component\Jed\Administrator\Field">
    <fieldset>
        <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
        <field name="contact_fullname" filter="safehtml" type="text" required="true" message="Please enter your name."
               label="COM_JED_GENERAL_FIELD_CONTACT_FULLNAME_LABEL"
               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_CONTACT_FULLNAME_DESCR"
               hint="COM_JED_GENERAL_FIELD_CONTACT_FULLNAME_LABEL"/>
        <field name="contact_organisation" filter="safehtml" type="text" required="true"
               message="Please enter your organisation / company name"
               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_CONTACT_ORGANISATION_LABEL"
               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_CONTACT_ORGANISATION_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_CONTACT_ORGANISATION_LABEL"/>
