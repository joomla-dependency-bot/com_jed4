               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_VULNERABLE_ITEM_VERSION_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_VULNERABLE_ITEM_VERSION_LABEL"/>
        <field name="extension_update" filter="safehtml" type="text"
               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_EXTENSION_UPDATE_LABEL"
