               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_UPDATE_DATE_SUBMITTED_LABEL"
               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_UPDATE_DATE_SUBMITTED_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_UPDATE_DATE_SUBMITTED_LABEL"/>
