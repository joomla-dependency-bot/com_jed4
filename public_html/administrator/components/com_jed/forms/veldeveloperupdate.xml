               description="COM_JED_VEL_DEVELOPERUPDATES_FIELD_UPDATE_NOTICE_URL_DESCR"
               hint="COM_JED_VEL_DEVELOPERUPDATES_FIELD_UPDATE_NOTICE_URL_LABEL"/>
        <field name="changelog_url" validate="url" type="url" required="true"
               label="COM_JED_VEL_DEVELOPERUPDATES_FIELD_CHANGELOG_URL_LABEL"
